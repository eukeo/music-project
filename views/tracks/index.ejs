<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script defer src="/public/javascripts/soundcloud.js"></script>
    <link rel="stylesheet" href='/stylesheets/style.css'/>
    <title>Wubble</title>
</head>
<body>
    <nav>
        <ul>
          <li><img src="https://worldwaterskiers.com/wp-content/themes/WorldWaterSkiers/downloads/logos/W-Icon-White-Circle.png" class="corner"></li>
          <li><a href="/tracks"><img src="https://www.pngkit.com/png/full/31-316559_white-home-icon-no-background.png" class="home-logo"></a></li>
          <li><a href="/logout"><img src="https://i.imgur.com/pN55Y25.png" class="logout-logo"></a></li>
          <li><a href="https://soundcloud.com/you/likes"><img src="https://a-v2.sndcdn.com/assets/images/peace-cloud-28ad0963.svg" class="soundcloud-logo"></a></li>
        </ul>
    </nav>
    <tr>
        <div class="container"></div>
            <div class="link">
                <form action="/tracks" method="POST">
                    <label>LINK:
                        <input type="Url" name="addTrack" id="link">
                    </label>
            </div>
            <div class="status">
                    <label>STATUS:
                        <input type="text" name="status" id="status">
                    </label>
                    <input type="submit" value="Upload">
                </form>
            </div>
            <section>
            <% tracks.forEach(function(track) { %>
                <td><%= track.addTrack %></td>
                <td><%= track.status %></td>
                <td><%= track.addComment %></td>
                <a href="/tracks/<%= track._id %>/comments" class="addComment" name="addComment">Comments</a>
                <% }) %>
            </section>
            </div>
    </tr>
</body>
</html>
